<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <h2>Sequalize Users</h2>
    <ul>
      <% data.forEach(user => { %>
      <li>
        ID: <%= user.dataValues.id %><br />
        User ID: <%= user.dataValues.userid %><br />
        Name: <%= user.dataValues.name %><br />
        Password: <%= user.dataValues.pw %>
      </li>
      <% }); %>
    </ul>
  </body>
</html>
